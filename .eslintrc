{
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jquery": true
  },
  "globals": {
    "theme": true,
    "slate": true,
    "timber": false,
    "QRCode": false,
    "google": true,
    "_": false
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true
      }
    ],
    "eqeqeq": [
      "error",
      "always"
    ]
  }
}